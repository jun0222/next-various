{
  "name": "next-various",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prod": "echo '=============================' && echo '===※ソース変更時は再実行！===' && echo '=============================' && next build && next start",
    "check-types": "tsc --noEmit",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts {pages,components}/**/*",
    "lint:fix": "eslint \"{pages,components}/**/*.{ts,tsx}\" --fix",
    "fmt": "prettier --write {pages,components}/**/*",
    "test-all": "npx yarn-run-all lint check-types format lint:fix",
    "lints": "yarn fmt && yarn lint:fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:clear-cache": "jest --clearCache",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:ci:watch": "jest --ci --watch",
    "test:ci:coverage": "jest --ci --coverage",
    "test:ci:coverage:upload": "jest --ci --coverage && codecov",
    "test:ci:coverage:upload:watch": "jest --ci --coverage && codecov --watch",
    "test:ci:coverage:upload:watch:all": "jest --ci --coverage && codecov --watch --files \"**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/d3": "^7.4.0",
    "@types/react-beautiful-dnd": "^13.1.5",
    "@types/react-dom": "^18.2.7",
    "axios": "^0.26.1",
    "csv": "^6.2.8",
    "d3": "4",
    "fetch-mock": "^9.11.0",
    "framer-motion": "^10.16.4",
    "fs": "^0.0.1-security",
    "jest": "^29.5.0",
    "next": "^13.4.10",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-d3-tree": "^3.5.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.43.9",
    "styled-components": "^6.0.8"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.1",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@types/jest": "^29.5.3",
    "@types/node": "17.0.21",
    "@types/react": "^18.2.18",
    "@types/testing-library__jest-dom": "^5.14.8",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "ignore-loader": "^0.1.2",
    "jest-environment-jsdom": "^29.6.1",
    "msw": "^1.3.2",
    "node-fetch": "2",
    "prettier": "2.6.0",
    "ts-jest": "^29.1.1",
    "typescript": "^4.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
